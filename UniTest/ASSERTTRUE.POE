FUNCTION assertTrue : BOOL
VAR_INPUT
	condition : BOOL; (*Variable to test*)
END_VAR

VAR

END_VAR
(** {*https://github.com/tkucic/UniTest/blob/main/docs/docs/ns/UniTest/fc/assertTrue.md*} **)
(** assertTrue := condition; **)
	(* assign - Stmt *)
	LD condition
	ST assertTrue

(** RETURN; **)
	(* return - Stmt *)
	RET 


END_FUNCTION